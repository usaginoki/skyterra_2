# Dataset configuration
DATASET_PATH=./data
REPO_ID=ibm-nasa-geospatial/multi-temporal-crop-classification
CACHE_DIR=./cache
OUT_DIR=./multicrop_example

# Training parameters
BATCH_SIZE=16
EPOCHS=50
LR=2.0e-4
WEIGHT_DECAY=0.1
HEAD_DROPOUT=0.1
FREEZE_BACKBONE=False

# Model parameters
BANDS=BLUE,GREEN,RED,NIR_NARROW,SWIR_1,SWIR_2
NUM_FRAMES=3

# Class weights (comma-separated)
CLASS_WEIGHTS=0.386375,0.661126,0.548184,0.640482,0.876862,0.925186,3.249462,1.542289,2.175141,2.272419,3.062762,3.626097,1.198702

# Model configuration
BACKBONE=prithvi_eo_v2_300_tl
BACKBONE_PRETRAINED=True
BACKBONE_COORDS_ENCODING=time,location

# Decoder configuration
DECODER=UperNetDecoder
DECODER_CHANNELS=256
DECODER_SCALE_MODULES=True

# Neck indices (comma-separated)
# For prithvi_eo_v2_300: 5,11,17,23
# For prithvi_eo_v2_600: 7,15,23,31
# For prithvi_vit_100: 2,5,8,11
NECK_INDICES=5,11,17,23

# Training configuration
SEED=0
NUM_WORKERS=7
PRECISION=bf16-mixed
LOG_EVERY_N_STEPS=10
